{
  "builds": [
    {
      "src": "'chat_app/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" , "runtime": "python3.9" }
    },
    {
      "src": "vercel_build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "chat_app/wsgi.py"
    }
  ]
}
